{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "WorkFlow": "Data Source=#{Common.DatabaseServer}#;Initial Catalog=#{Common.WorkFlow.DatabaseName}#;Integrated Security=True"
  },
  "IccPortalConfig": {
    "Jwt": { 
      "Secret": "#{Deploy.IccPortal.JWTSecret}#"
    },
    "FrontendBaseUrl": "#{Deploy.IccPortal.Hostname}#",
    "IdentityHub": {
      "BaseUrl": "#{Deploy.IccPortal.BaseURL}#",
      "Tenant": "#{Deploy.IccPortal.Tenant}#",
      "ClientId": "#{Deploy.IccPortal.ClientID}#",
      "ClientSecret": "#{Deploy.IccPortal.ClientSecret}#"
    }
  }
}
